{
	"name": "rob006/yii2-simple-auth-yii-authenticator",
	"description": "Yii 2 extension that provides helper for authenticate Request from yii2-httpclient package.",
	"type": "yii2-extension",
	"keywords": ["yii2", "extension", "auth", "authentication", "authenticator"],
	"license": "MIT",
	"authors": [
		{
			"name": "Robert Korulczyk",
			"email": "robert@korulczyk.pl",
			"homepage": "https://rob006.net/"
		}
	],
	"support": {
		"issues": "https://github.com/rob006/yii2-simple-auth-yii-authenticator/issues",
		"source": "https://github.com/rob006/yii2-simple-auth-yii-authenticator"
	},
	"require": {
		"yiisoft/yii2": "2.0.*",
		"yiisoft/yii2-httpclient": "~2.0.1",
		"rob006/yii2-simple-auth": "1.0.x-dev"
	},
	"autoload": {
		"psr-4": {
			"rob006\\simpleauth\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
}
